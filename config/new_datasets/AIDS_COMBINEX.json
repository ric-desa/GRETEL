{  
    "experiment" : {
        "scope": "examples_configs",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections" : ["explainers"],"params" : {"fold_id": 0}},
                {"in_sections" : ["do-pairs/oracle"],"params" : {"fold_id": -1}},
                {"in_sections": ["do-pairs/dataset"],"params": { "compose_man" : "config/snippets/datasets/centr_and_weights.json" }}
            ]
        }
    },
    "do-pairs": [ 
        {
            "dataset": {
                "class": "src.dataset.dataset_base.Dataset",
                "parameters": {
                    "generator": {
                        "class": "src.dataset.generators.TUDataset.TUDataset",
                        "parameters": { "alias": "AIDS" }
                    }
                }
            },
            "oracle": {
                "class": "src.oracle.nn.torch.OracleTorch",
                "parameters": {
                    "epochs": 500,
                    "batch_size": 128,
                    "optimizer": {
                        "class": "torch.optim.RMSprop",
                        "parameters": {
                            "lr":0.01                
                        }
                      },
                    "loss_fn": {
                        "class": "torch.nn.CrossEntropyLoss",
                        "parameters": {     
                          "reduction":"mean"
                        }
                      },
                    "model": { 
                      "class": "src.oracle.nn.gcn.DownstreamGCN",
                      "parameters": {"num_conv_layers":2,"num_dense_layers":1,"conv_booster":3,"linear_decay":1.8}
                  } 
                }   
            }
        }
        ],
    "explainers": [
        {
            "class": "src.explainer.learned.combinex.COMBINEX",
            "parameters": {
                "optimizer": {
                    "name": "adam",
                    "lr": 0.01,
                    "num_epochs": 100
                },
                "scheduler": {
                    "policy": "linear",
                    "initial_alpha": 0.5,
                    "decay_rate": 10.0
                },
                "timeout": 3600,
                "seed": 42
            }
        }
    ],
    "compose_mes" : "config/snippets/default_metrics.json",
    "compose_strs" : "config/snippets/default_store_paths.json"
}

